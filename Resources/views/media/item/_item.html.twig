        }
    ) }}" data-toggle="modal" data-target="#appModal" data-title="{{ 'label.user.media.photo'|trans(
        {
            '%username%': '@' ~ user_media.createdBy.username,
        }
    ) }}">
        {#
        {{ include('@UserMedia/media/item/_item_media.html.twig',
            {
                'user_media': user_media,
            },
            with_context = false
        ) }}
        #}
    </a>
    {#
    {{ include('media/item/_control.html.twig',
        {
            'user_media': user_media,
            'user_media_social': user_media_social ?? null,
            'count_view': count_view ?? null,
        },
        with_context = false
    ) }}
    #}
